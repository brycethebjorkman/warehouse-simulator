target C

reactor Retrieval {
    input pkg_in: int
    input cmd: int  
    output msg: int
    output pkg_out: int
    state c: int = 0
    timer t(0, 1 sec)
    reaction(pkg_in) {=
        lf_print("Package Received: %d at (%lld, %d)", pkg_in->value,
            lf_time_logical_elapsed(), lf_tag().microstep
        );
    =}
    reaction(cmd) {=
        lf_print("Command Received: %d at (%lld, %d)", cmd->value,
            lf_time_logical_elapsed(), lf_tag().microstep
        );
    =}
}
